<div id="simpledialog" class="dialog-main" show="{{diashow}}">
    <div class="dialog-div">
        <div class="inner-txt">
            <text class="txt">{{content}}</text>
        </div>
        <div class="inner-btn">
            <button focusable="false" type="capsule" value="关闭" onclick="cancelSchedule" class="btn-txt"></button>
            <button focusable="false" type="capsule" value="确定" onclick="setSchedule" class="btn-txt"></button>
        </div>
    </div>
</div>
